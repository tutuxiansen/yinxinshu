<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhhy.dao.ReserveTalentDao">
	
	<resultMap id="reser" type="com.xhhy.domain.ReserveTalentBean">
		<id property="dataId" column="data_id" />
		<result property="empId" column="emp_id" />
		<result property="empName" column="emp_name" />
		<result property="empGender" column="emp_gender" />
		<result property="empTel" column="emp_tel" />
		<result property="empProfession" column="emp_profession" />
		<result property="empEducation" column="emp_education" />
		<result property="empScholl" column="emp_scholl" />
		<result property="empWorkExperience" column="emp_workExperience" />
		<result property="empAppointed" column="emp_appointed" />
	</resultMap>

	
	<sql id="all">
		data_id,emp_id,emp_name,emp_gender,emp_tel,emp_profession,emp_education,
		emp_scholl,emp_workExperience,emp_appointed
	</sql>

	<select id="queryAll" resultMap="reser">
		select 
		<include refid="all"/>
		from emp_reserveTalent_tb
	</select>
	
	<select id="queryByName" parameterType="ReserveTalentBean"  resultMap="reser">
		select 
		<include refid="all"/>
		from emp_reserveTalent_tb  where  emp_name like '%${empName}%'
	</select>
	
</mapper>
        